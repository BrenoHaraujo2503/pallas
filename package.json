{
  "name": "todo-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "npm run build && concurrently \"npm run build:watch\" \"npm run start:watch\"",
    "start:watch": "nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "dotenv -e .env.test -- node database-test-connection.mjs && dotenv -e .env.test -- npm run prisma:deploy && dotenv -e .env.test -- jest -i -c jest-e2e.config.js",
    "build": "swc src --no-swcrc --config-file .swcrc.build -d dist",
    "build:watch": "swc src -w --no-swcrc --config-file .swcrc.build -d dist",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:migrate": "prisma migrate dev",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.9",
    "@swc/jest": "^0.2.23",
    "@types/node": "^18.11.0",
    "concurrently": "^7.4.0",
    "dotenv-cli": "^6.0.0",
    "jest": "^29.2.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.4.0",
    "supertest": "^6.3.0"
  }
}
